    {
      "description": "a POST request for bulk current weather",
      "providerStates": [
        {
          "name": "bulk request on free plan"
        }
      ],
      "request": {
        "body": {
          "locations": [
            {
              "custom_id": "l1",
              "q": "London"
            },
            {
              "custom_id": "z1",
              "q": "90201"
            }
          ]
        },
        "headers": {
          "Accept": "application/json",
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/v1/current.json",
        "query": {
          "key": [
            "PACT-KEY-12345"
          ],
          "q": [
            "bulk"
          ]
        }
      },
      "response": {
        "body": {
          "error": {
            "code": 2009,
            "message": "API key does not have access to the resource. Please check your plan and billing details."
          }
        },
        "headers": {
          "Accept": "application/json",
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.error.code": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            },
            "$.error.message": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {},
          "status": {}
        },
        "status": 400
      }
    },
